{
	"family": "spring_demo",
	"containerDefinitions": [
		{
			"image": "661382096004.dkr.ecr.us-east-1.amazonaws.com/spring_demo:v_%BUILD_NUMBER%",
			"name": "spring_demo",
			"cpu": 1024,
			"memory": 1024,
			"essential": true,
			"portMappings": [
				{
					"containerPort": 8080,
					"hostPort": 80
				}
			],
            "logConfiguration": {
                "logDriver": "awslogs",
                "options": {
                    "awslogs-group": "/app/plat/spring_demo",
                    "awslogs-region": "us-east-1",
                    "awslogs-stream-prefix": "staging/bingo"
                }
            }
		}
	]
}